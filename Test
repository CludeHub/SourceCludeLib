local Clude = {}

function Clude:CreateWindow(config)
    local gui = Instance.new("ScreenGui")
    gui.Name = "Clude"
    gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    gui.ResetOnSpawn = true

    local window = Instance.new("Frame")
    window.Name = "Window"
    window.Parent = gui
    window.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    window.Active = true
    window.Draggable = true
    window.Size = UDim2.new(0.485, 0, 0.885, 0)
    window.Position = UDim2.new(0.340, 0, 0, 0)

    local uiCorner = Instance.new("UICorner")
    uiCorner.CornerRadius = UDim.new(0, 5)
    uiCorner.Parent = window

    local TabFrams = Instance.new("Frame")
    TabFrams.Name = "TabFrams"
    TabFrams.Parent = window
    TabFrams.BackgroundColor3 = Color3.fromRGB(44, 47, 51)
    TabFrams.BackgroundTransparency = 0.2
    TabFrams.Size = UDim2.new(0.347, 0, 1, 0)
    TabFrams.Position = UDim2.new(-0.348, 0, 0, 0)

    local uiCorner1 = Instance.new("UICorner")
    uiCorner1.CornerRadius = UDim.new(0, 5)
    uiCorner1.Parent = TabFrams

    local label1 = Instance.new("TextLabel")
    label1.Parent = TabFrams
    label1.BackgroundColor3 = Color3.fromRGB(44, 47, 51)
    label1.Text = config.Title
    label1.Size = UDim2.new(0.9, 0, 0.20, 0)
    label1.Position = UDim2.new(0.039, 0, -0.04, 0)
    label1.TextColor3 = Color3.fromRGB(255, 255, 255)
    label1.TextScaled = true
    label1.Font = Enum.Font.SourceSansBold
    label1.BackgroundTransparency = 1
end

return Clude
